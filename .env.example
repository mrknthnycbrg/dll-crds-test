APP_NAME=DLL-CRDS
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost # Use 'http://localhost' if using Laravel Sail or use 'http://127.0.0.1:8000' if using XAMPP.
APP_SERVICE=dll-crds-test
APP_TIMEZONE='Asia/Manila'

LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
