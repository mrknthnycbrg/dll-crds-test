MEMCACHED_HOST=127.0.0.1

REDIS_HOST=redis # Use 'redis' if using Laravel Sail or use '127.0.0.1' if not.
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=smtp
MAIL_HOST=localhost
MAIL_PORT=1025
MAIL_USERNAME=null
MAIL_PASSWORD=null
